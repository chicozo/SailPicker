<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="doran.sailpicker"
    >
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone_sail_picker"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".PremierePage" android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity android:name=".MenuPlanche" android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity android:name=".NouveauStagiaireMenu" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".TraitementPlanche" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".Zoom" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".TraitementNouveauStagiaire" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>
        <activity android:name=".About" android:theme="@style/Theme.AppCompat.Light.NoActionBar"></activity>

    </application>

</manifest>